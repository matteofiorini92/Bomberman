        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_LgMycFxOEe6uSKrMMqJ3zQ" name="loadStringClassProperties" specification="_LeCPQFxOEe6uSKrMMqJ3zQ">
