          <ownedComment xmi:id="_LgftZFxOEe6uSKrMMqJ3zQ">
            <body>welcome board to select existing or new player&#xD;
@author Matteo&#xD;
 *</body>
          </ownedComment>
          <generalization xmi:id="_Ld66glxOEe6uSKrMMqJ3zQ" general="_Lbi78VxOEe6uSKrMMqJ3zQ"/>
          <ownedAttribute xmi:id="_LfJCgFxOEe6uSKrMMqJ3zQ" name="PLAYER_PROFILE_FOLDER" isLeaf="true" isStatic="true" type="_Lbw-YFxOEe6uSKrMMqJ3zQ" isReadOnly="true" association="_Lg_coVxOEe6uSKrMMqJ3zQ">
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_LfJCgVxOEe6uSKrMMqJ3zQ" value="Path.of(&quot;resources/playerProfiles&quot;)"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_LfJCglxOEe6uSKrMMqJ3zQ" name="PROFILE_BUTTON_HEIGHT" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_LfJCg1xOEe6uSKrMMqJ3zQ" value="50.0"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_LfJpkFxOEe6uSKrMMqJ3zQ" name="PROFILE_BUTTON_WIDTH" isLeaf="true" isStatic="true" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_LfJpkVxOEe6uSKrMMqJ3zQ" value="300.0"/>
