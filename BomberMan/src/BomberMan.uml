        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_LgBMRFxOEe6uSKrMMqJ3zQ" name="processKeyPress" specification="_Ld0M0FxOEe6uSKrMMqJ3zQ">
          <ownedParameter xmi:id="_LgBMRVxOEe6uSKrMMqJ3zQ" name="event" type="_LcBdEFxOEe6uSKrMMqJ3zQ"/>
          <language>java</language>
          <body>long currentTime = System.currentTimeMillis();&#xD;
&#xD;
	    if (currentTime - lastKeyPressTime >= throttleDelay) {&#xD;
	    	lastKeyPressTime = currentTime;&#xD;
	    	model.BomberMan modelBm = model.BomberMan.getInstance();&#xD;
	    	if (event.getCode() == KeyCode.SPACE &amp;&amp; &#xD;
	    		modelBm.getBombs() > 0 &amp;&amp; &#xD;
	    		!(model.GameBoard.getInstance().getCell(modelBm.getPosition()) instanceof model.Bomb) &amp;&amp;&#xD;
	    		!(modelBm.getTempStorage() instanceof model.Exit)&#xD;
	    	) {&#xD;
	    		view.BaseGroup baseGroup = view.BaseGroup.getInstance();&#xD;
		    	model.Bomb modelBomb = new model.Bomb(modelBm.getPosition());&#xD;
		    	view.Bomb viewBomb = new view.Bomb();&#xD;
		    	modelBomb.addObserver(viewBomb);&#xD;
		    	modelBm.decBombs();&#xD;
		    	modelBm.setTempStorage(modelBomb);&#xD;
		    	// get viewBm stackPane index in order to add the bomb behind it&#xD;
		    	int viewBmStackPaneIndex = baseGroup.getChildren().indexOf(viewBoard.getGridPane());&#xD;
		    	&#xD;
		    	view.GameBoard.getInstance().getItemsPane().getChildren().add(viewBmStackPaneIndex + 1, viewBomb);&#xD;
		    	modelBomb.trigger();	        	&#xD;
		    } else if (event.getCode() == KeyCode.P) {&#xD;
		    	model.GameBoard.getInstance().print();&#xD;
		    } else if (event.getCode() == KeyCode.F) {&#xD;
		    	model.GameBoard.getInstance().printExt();&#xD;
		    } &#xD;
		    else {	        	&#xD;
		        	Direction direction = null;&#xD;
		        	try {	        	&#xD;
		        		switch (event.getCode()) {&#xD;
		        		case DOWN:&#xD;
		        			direction = Direction.DOWN;&#xD;
		        			break;&#xD;
		        		case RIGHT:&#xD;
		        			direction = Direction.RIGHT;&#xD;
		        			break;&#xD;
		        		case UP:&#xD;
		        			direction = Direction.UP;&#xD;
		        			break;&#xD;
		        		case LEFT:&#xD;
		        			direction = Direction.LEFT;&#xD;
		        			break;&#xD;
		        		default:&#xD;
		        			break;	&#xD;
		        		}&#xD;
		        		modelBm.move(direction);	        		&#xD;
		        	} catch (NullPointerException e) {&#xD;
		        		System.out.println(&quot;Invalid command&quot;);&#xD;
		        	}&#xD;
	        }&#xD;
	    }&#xD;
&#xD;
	    if (keyHeld) {&#xD;
	    	Platform.runLater(() -> processKeyPress(event));&#xD;
	    }&#xD;
	</body>
        </ownedBehavior>
