        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_LgQc0lxOEe6uSKrMMqJ3zQ" name="getInstance" specification="_LSlNMVxOEe6uSKrMMqJ3zQ">
          <ownedParameter xmi:id="_LgQc01xOEe6uSKrMMqJ3zQ" type="_LSjYAFxOEe6uSKrMMqJ3zQ" direction="return"/>
          <language>java</language>
          <body>if (timer == null) {&#xD;
