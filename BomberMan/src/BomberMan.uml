        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Lf53glxOEe6uSKrMMqJ3zQ" name="_initializer1" specification="_LQPq4FxOEe6uSKrMMqJ3zQ">
          <language>java</language>
          <body>modelEnemies.put(&quot;he&quot;, model.Helix.class);&#xD;
		modelEnemies.put(&quot;bug&quot;, model.Bug.class);&#xD;
		viewCharacters.put(&quot;he&quot;, view.Helix.class);&#xD;
		viewCharacters.put(&quot;bug&quot;, view.Bug.class);&#xD;
	</body>
