        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Lf_XEFxOEe6uSKrMMqJ3zQ" name="Level" specification="_LQ_RwFxOEe6uSKrMMqJ3zQ">
          <ownedParameter xmi:id="_Lf_XEVxOEe6uSKrMMqJ3zQ" name="level">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
