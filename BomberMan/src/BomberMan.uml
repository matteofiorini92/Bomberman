        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_LgYYqFxOEe6uSKrMMqJ3zQ" name="update" specification="_LeilkFxOEe6uSKrMMqJ3zQ">
          <ownedParameter xmi:id="_LgYYqVxOEe6uSKrMMqJ3zQ" name="o" type="_Lb6IUFxOEe6uSKrMMqJ3zQ"/>
          <ownedParameter xmi:id="_LgYYqlxOEe6uSKrMMqJ3zQ" name="arg" type="_Ldv7YFxOEe6uSKrMMqJ3zQ"/>
          <ownedParameter xmi:id="_LgYYq1xOEe6uSKrMMqJ3zQ" name="imageFiles" type="_LbiU4VxOEe6uSKrMMqJ3zQ"/>
          <language>java</language>
          <body>Object[] args = (Object[]) arg;&#xD;
		&#xD;
		if (args[0].equals(model.ChangeType.MOVE)) {			&#xD;
			move((model.Character)o, imageFiles, args[1]);&#xD;
		}&#xD;
		else if (args[0].equals(model.ChangeType.BECOME_INVINCIBLE)) {&#xD;
			flash();&#xD;
		}&#xD;
		else if (args[0].equals(model.ChangeType.DIE)) {&#xD;
			die((model.Character)o, imageFiles);&#xD;
		}&#xD;
		else if (args[0].equals(model.ChangeType.CHANGE_SPEED)) {&#xD;
			changeSpeed((Double)args[1]);&#xD;
		}&#xD;
	</body>
